<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.west.es.context.contextModel.presentation.ContextModelModelWizardID"
            name="%_UI_ContextModelModelWizard_label"
            class="org.west.es.context.contextModel.presentation.ContextModelModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/ContextModelModelFile.gif">
         <description>%_UI_ContextModelModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="org.west.es.context.contextModel.presentation.ContextModelEditorID"
            name="%_UI_ContextModelEditor_label"
            icon="icons/full/obj16/ContextModelModelFile.gif"
            extensions="contextmodel"
            class="org.west.es.context.contextModel.presentation.ContextModelEditor"
            contributorClass="org.west.es.context.contextModel.presentation.ContextModelActionBarContributor">
      </editor>
   </extension>

</plugin>
